# Local development settings
REACT_APP_LOCAL_DEV=false

# Optional: JWT token for local development (when REACT_APP_LOCAL_DEV=true)
# To get a token: 
# 1. Set REACT_APP_LOCAL_DEV=false
# 2. Log in through Mozilla Auth
# 3. Copy the id_token from browser DevTools (Network tab)
REACT_APP_DEV_JWT_TOKEN=

# API endpoint (optional, defaults to production)
REACT_APP_ADMIN_API_ENDPOINT=https://admin-api.getpocket.dev
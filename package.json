    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "5.13",
    "@mui/system": "^5.12.3",
    "@mui/x-date-pickers": "^5.0.20",
    "@sentry/react": "^10.17.0",
    "@sentry/tracing": "6.17.9",
    "@types/apollo-upload-client": "18.0.0",
    "@types/luxon": "2.0.9",
    "@types/react-beautiful-dnd": "13.1.4",
    "@types/react-dom": "17.0.20",
    "@types/react-router-dom": "5.3.3",
    "apollo-upload-client": "18.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^30.2.0",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "10.0.1",
    "browserslist": "^4.26.3",
    "buffer": "^6.0.3",
    "camelcase": "^6.1.0",
    "crypto-browserify": "^3.12.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-testing-library": "6.2.2",
    "formik": "2.2.9",
    "fs-extra": "^9.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "jest-resolve": "26.6.0",
    "jest-watch-typeahead": "^3.0.1",
    "luxon": "1.28.1",
    "notistack": "2.0.8",
    "process": "^0.11.10",
    "react": "17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "17.0.2",
    "react-dropzone": "11.5.3",
    "react-markdown": "6.0.3",
    "react-oauth2-pkce": "2.0.7",
    "react-refresh": "^0.14.2",
    "react-router-dom": "5.3.4",
    "resolve": "1.18.1",
    "semver": "7.6.0",
    "slugify": "1.6.6",
    "stream-browserify": "^3.0.0",
    "typescript": "4.9.5",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "web-vitals": "2.1.4",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:nowatch": "jest --watchAll=false",
    "lint-check": "eslint src/**/*.ts{,x}",
    "lint-fix": "eslint src/**/*.ts{,x} --fix",
    "api:generate-types": "graphql-codegen --config src/config/admin-api-codegen.json -r dotenv/config"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/config/jest/setupEnv.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(apollo-upload-client|@apollo/client|extract-files|is-plain-obj)/)",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "watchman": false,
    "resetMocks": true,
    "globalSetup": "./jest.global-setup.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "13.5.0",
    "@types/chai": "4.3.5",
    "@types/jest": "29.5.3",
    "@types/node": "18.19.31",
    "@types/react": "17.0.62",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "ajv": "^8.17.1",
    "chai": "4.3.7",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "husky": "^8.0.1",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "3.3.0",
    "react-error-overlay": "6.0.11",
    "vite": "^5.4.2",
    "vite-plugin-node-polyfills": "^0.21.0"
  },
  "overrides": {
    "ws": "^8.18.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
